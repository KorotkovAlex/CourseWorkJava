<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>log</from-outcome>
            <to-view-id>/Log/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>updateProduct</from-outcome>
            <to-view-id>/Admin/updateProduct.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addProduct</from-outcome>
            <to-view-id>/Admin/addProduct.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteProduct</from-outcome>
            <to-view-id>/Admin/deleteProduct.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addUser</from-outcome>
            <to-view-id>/Admin/addUser.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/addProduct.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/Admin/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/updateProduct.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/Admin/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/listUsers.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>updateUser</from-outcome>
            <to-view-id>/Admin/updateUser.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/updateUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listUsers</from-outcome>
            <to-view-id>/Admin/listUsers.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
